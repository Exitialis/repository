{
    "name": "exitialis/repository",
    "description": "Repository pattern for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Exitialis",
            "email": "lpexitialis@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "^5.3.0",
        "illuminate/support": "^5.3.0",
        "hautelook/phpass": "^1.0"
    },
    "require-dev": {
        "laravel/laravel": "dev-develop",
        "phpunit/phpunit": "^5.5",
        "orchestra/testbench": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Exitialis\\Repository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Exitialis\\Repository\\Tests\\": "tests/"
        }
    }
}
